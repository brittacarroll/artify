
<%= form_for [@artist, @artwork], html: { multipart: true } do |f| %>

  <div class="field">
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </div>

  <div class="field">
    <%= f.label :description %><br>
    <%= f.text_field :description %>
  </div>

  <div class="field">
    <%= f.label :art, "Artwork" %><br>
    <%= f.file_field :art %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>

<%= link_to "Back", artists_profile_path(@profile) %>
